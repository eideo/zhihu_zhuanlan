<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">      <!-- 默认背景-->
        <shape>
            <corners android:radius="3dip"/>
            <solid android:color="#336f7a7e"/>
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">      <!-- 第二进度背景-->
        <clip>
            <shape>
                <corners android:radius="5dip"/>
                <solid android:color="#6f7a7e"/>
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">            <!-- 进度背景-->
        <clip>
            <shape>
                <corners android:radius="3dip"/>
                <solid android:color="#6683cb3b"/>
            </shape>
        </clip>
    </item>
</layer-list>